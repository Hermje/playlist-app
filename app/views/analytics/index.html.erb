<nav class="navbar navbar-dark shadow">
  <a href="/analytics/index" class="navbar-brand mb-0 h1">Site Analytics</a>
</nav>

<div class="data-container">
  <h3>Timelines</h3>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">User</th>
        <th scope="col">Subscribers</th>
        <th scope="col">View</th>
        <th scope="col">Destroy</th>
      </tr>
    </thead>
    <tbody>
      <% @all_tls.each_with_index do |t, i| %>
        <tr>
          <td><%= i+1 %></td>
          <td><%= t.creator %></td>
          <td><%= t.subscribers.split(',').length %></td>
          <td><%= link_to "timeline" %></td>
          <td><%= link_to "destroy", {:controller => 'analytics', :action => 'destroy', :user => t.creator} %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
